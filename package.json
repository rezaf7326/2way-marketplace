    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "fast-safe-stringify": "^2.1.1",
    "http-errors": "^2.0.0",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.13.0",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.4",
    "sequelize-typescript": "^2.1.6",
    "winston": "^3.15.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
