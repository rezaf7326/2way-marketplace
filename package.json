{
  "name": "2way-marketplace",
  "version": "1.0.0",
  "main": "main.js",
  "scripts": {
    "build": "tsc",
    "start": "ts-node src/main.ts",
    "start:dev": "nodemon src/main.ts",
    "start:prod": "node dist/main.js",
    "migration:generate": "npx sequelize-cli migration:generate --migrations-path ./src/database/migrations --name new-migration",
    "migration:run": "npm run build && npx sequelize-cli db:migrate --migrations-path ./dist/database/migrations --config ./dist/database/config.js"
  },
